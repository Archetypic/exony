                    resourcet >= 1.1 && < 1.2,
-                   text >= 1.1 && < 1.2,
-                   time >= 1.4 && < 1.5,
-                   transformers >= 0.3 && < 0.4,
+                   text >= 1.1 && < 1.3,
+                   time >= 1.4 && < 1.6,
+                   transformers >= 0.3 && < 0.5,
                    transformers-base >= 0.4 && < 0.5,
                    vector >= 0.10 && < 0.11
